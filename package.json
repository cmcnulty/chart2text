{
  "name": "chart2text",
  "version": "1.2.0",
  "description": "Chart.js plugin that generates natural language descriptions of chart data for screen readers",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "demo": "npx http-server -p 8080",
    "prepublishOnly": "npm run build",
    "predeploy": "npm run build && node scripts/prepare-demo.js",
    "deploy": "gh-pages -d docs --dotfiles --add"
  },
  "keywords": [
    "chartjs",
    "chart.js",
    "accessibility",
    "a11y",
    "screen-reader",
    "aria",
    "wcag",
    "visualization",
    "plugin"
  ],
  "author": "Charles McNulty",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmcnulty/chart2text.git"
  },
  "bugs": {
    "url": "https://github.com/cmcnulty/chart2text/issues"
  },
  "homepage": "https://cmcnulty.github.io/chart2text",
  "peerDependencies": {
    "chart.js": ">=3.0.0"
  },
  "dependencies": {
    "segreg": "^0.5.5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "chart.js": "^4.4.1",
    "eslint": "^8.56.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "rollup": "^4.9.6",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
